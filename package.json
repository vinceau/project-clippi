{
  "name": "project-clippi",
  "version": "0.0.1",
  "description": "Slippi automation framework",
  "license": "MIT",
  "build": {
    "mac": {
      "category": "public.app-category.lifestyle"
    },
    "win": {
      "target": [
        "portable"
      ]
    },
    "dmg": {
      "icon": false
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Office"
    }
  },
  "scripts": {
    "start": "electron-webpack dev",
    "build": "electron-webpack",
    "dist": "yarn build && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "dist:all": "yarn run dist --x64 -wml",
    "dist:win": "yarn run dist --x64 -w",
    "dist:mac": "yarn run dist -m",
    "dist:linux": "yarn run dist -l"
  },
  "dependencies": {
    "@rematch/core": "^1.3.0",
    "@rematch/persist": "^1.1.6",
    "@vinceau/slp-realtime": "^0.0.3",
    "electron-context-menu": "^0.15.1",
    "final-form": "^4.18.6",
    "final-form-arrays": "^3.0.2",
    "glob": "^7.1.6",
    "immer": "^5.0.1",
    "open": "^7.0.0",
    "rc-progress": "^2.5.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-final-form": "^6.3.3",
    "react-final-form-arrays": "^3.1.1",
    "react-redux": "^7.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^3.0.8",
    "redux": "^4.0.1",
    "source-map-support": "^0.5.10",
    "styled-components": "^4.4.1",
    "twitch": "^3.5.6",
    "twitch-electron-auth-provider": "^3.5.6"
  },
  "devDependencies": {
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.1.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-select": "^3.0.8",
    "@types/styled-components": "^4.4.1",
    "electron": "4.1.1",
    "electron-builder": "22.2.0",
    "electron-webpack": "^2.6.2",
    "electron-webpack-ts": "^3.1.1",
    "node-sass": "^4.11.0",
    "react-hot-loader": "4.6.5",
    "sass-loader": "^7.1.0",
    "ts-unused-exports": "^5.1.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.3.3333",
    "webpack": "4.35.3",
    "webpack-build-notifier": "^0.1.30"
  },
  "electronWebpack": {
    "title": "Project Clippi",
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    }
  }
}